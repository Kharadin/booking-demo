 navigate = useNavigate()\n\n\n\n\n     const handleOption = (optionName, operation) => {\n     \n     setOptions(objToChange=> {\n          return {\n          ...objToChange,\n          [optionName]: operation ==='i' ? options[optionName] + 1 : options[optionName] -1\n\n          };\n     });\n\n     };\n          \n  const handleSearch = () => {\n     navigate (\"/hotels\", {state:{destination, dates, options}})\n  }\n\nreturn (\n     <div className='header'>\n          <div className={type === \"list\" ? \"headerContainer listMode\" : \"headerContainer\"}> \n               <div className=\"headerList\">\n                         <div className=\"headerListItem active\">\n                         <FontAwesomeIcon icon={faBed} />\n                         <span>Stays</span>\n                         </div>\n                         <div className=\"headerListItem\">\n                         <FontAwesomeIcon icon={faPlane} />\n                         <span>Flights</span>\n                         </div>\n                         <div className=\"headerListItem\">\n                         <FontAwesomeIcon icon={faCar} />\n                         <span>Car Rentals</span>\n                         </div>\n                         <div className=\"headerListItem\">\n                              <FontAwesomeIcon icon={faBinoculars} />\n                              <span>Sigtseeings</span>\n                         </div>\n                              <div className=\"headerListItem\">\n                                   <FontAwesomeIcon icon={faTaxi} />\n                              \n                                   <span>Airport Taxi</span>\n                              </div>\n     </div>\n     \n     { type !== 'list' &&\n      <>\n     <h1 className=\"headerTitle\">A lifetime of discounts</h1>\n     <p className=\"headerDesc\">Get rewarded for your travels - unlock instant savings for 10% or more with a free Lamabooking account</p>\n          <button className=\"headerBtn\">Sign in / Register </button>\n          <div className=\"headerSearch\">\n               <div className=\"headerSearchItem\">\n                    <FontAwesomeIcon icon={faBed} className= \"headerIcon\" />\n                    <input className='headerSearchInput'\n                    type=\"text\"\n                    placeholder='Where are you going ?'\n                    onChange={e=>setDestination(e.target.value)}\n                         />\n               </div> \n          \n               <div className=\"headerSearchItem\">\n                    <FontAwesomeIcon icon={faCalendarDays} className= \"headerIcon\" />\n                    <span onClick= {()=> setOpenDate(!openDate)} className='headerSearchText'>{`${format(dates[0].startDate, \"MM/dd/yyyy\")} to ${format(dates[0].endDate, \"MM/dd/yyyy\")}`}</span>\n                    { openDate && <DateRange \n                         editableDateInputs={true} \n                              onChange= {item => setDates([item.selection])}\n                              moveRangeOnFirstSelection={false}\n                              ranges={dates}\n                              className='date'\n                              minDate={new Date()}\n                         />}\n               </div> \n          \n               <div className=\"headerSearchItem\">\n                    <FontAwesomeIcon icon={faPerson} className= \"headerIcon\" />\n                    <span onClick={()=>setOpenOptions(!openOptions)} \n                    className='headerSearchText'>{`${options.adult} adult · ${options.children} children · ${options.room} room`}</span>\n\n                    {openOptions && <div className= \"options\">\n                         <div className='optionItem'>\n                              \n                              <span className='optionText'>Adult\n                              </span>\n                                 <div className=\"optionCounter\">\n\n                                   <button \n                                   disabled={options.adult <= 1}\n                                   className='optionCounterButton' onClick={()=>handleOption(\"adult\", \"d\")}>-</button>\n                                   <span className=\"optionCounterNumber\">{options.adult} </span>\n                                   <button className='optionCounterButton' onClick={()=>handleOption(\"adult\", \"i\")}>+</button>\n                                 </div>\n                         </div>\n                         <div className='optionItem'>\n                              <span className='optionText'>Children\n                              </span>\n                              <div className=\"optionCounter\">\n                                   <button \n                                   disabled={options.children <= 0}\n                                   className='optionCounterButton' onClick={()=>handleOption(\"children\", \"d\")}>-</button>\n                                   <span className=\"optionCounterNumber\">{options.children}</span>\n                                   <button className='optionCounterButton' onClick={()=>handleOption(\"children\", \"i\")}>+</button>\n                              </div>     \n                         </div>\n                         <div className='optionItem'>\n                              <span className='optionText'>Room\n                              </span>\n                              <div className=\"optionCounter\">\n                                <button \n                                disabled={options.room <= 1}\n                                className='optionCounterButton' onClick={()=>handleOption(\"room\", \"d\")}>-</button>\n                                <span className=\"optionCounterNumber\"> {options.room} </span>\n                                <button className='optionCounterButton' onClick={()=>handleOption(\"room\", \"i\")}>+</button>\n                        